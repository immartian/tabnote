{
  "manifest_version": 2,
  "name": "TabNote",
  "version": "0.1",
  "icons": { "128": "tabnote.png" },
  "background": {
  "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js","epiceditor.min.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "tabnote.png"
  },
  "permissions": [
    "tabs"
  ]
}
